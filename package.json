{
    "name": "puttputtplanet",
    "type": "module",
    "version": "1.0.10",
    "description": "Putt Putt Planet website",
    "main": "bin/build.js",
    "scripts": {
      "build": "node bin/build.js",
      "clean": "rm -rf ./node_modules && rm -rf ./distrib && rm ./data/logs/*.log",
      "deploy": "node bin/deploy.js --config=bin/deploy-config.json",
      "sitemap": "node bin/sitemap.js",
      "test": "jest test",
      "versionup": "node bin/versionUp.js --debug --task build --path ./"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/VarynInc/puttputtplanet.git"
    },
    "keywords": [
      "minigolf",
      "website",
      "javascript",
      "games",
      "fun",
      "play"
    ],
    "author": "Varyn Inc.",
    "license": "ISC",
    "bugs": {
      "url": "https://github.com/VarynInc/puttputtplanet/issues"
    },
    "homepage": "https://puttputtplanet.com",
    "devDependencies": {
      "async": "^3.2.6",
      "axios": "^1.7.7",
      "chalk": "^4.1.2",
      "cheerio": "^1.0.0",
      "fs-extra": "^10.1.0",
      "glob": "^7.2.0",
      "gulp-rename": "^2.0.0",
      "imagemin": "^7.0.1",
      "jest": "^29.7.0",
      "os": "^0.1.2",
      "path": "^0.12.7",
      "pretty-bytes": "^5.6.0",
